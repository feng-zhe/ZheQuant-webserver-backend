{
  "name": "ZheQuant-webserver-backend",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "build": "gulp build-pro",
    "watch": "watchify src/javascripts/react/index.js -o public/build/javascripts/index_min.js -v"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "babel": {
    "plugins": [
      [
        "module-resolver",
        {
          "root": [
            "./"
          ]
        }
      ]
    ]
  },
  "dependencies": {
    "amqplib": "^0.5.1",
    "body-parser": "~1.15.2",
    "cookie-parser": "~1.4.3",
    "debug": "~2.2.0",
    "express": "^4.14.0",
    "jquery": "^3.1.1",
    "mongodb": "^2.2.22",
    "morgan": "~1.7.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "semantic-ui-react": "^0.61.10"
  },
  "devDependencies": {
    "babel-plugin-module-resolver": "^2.5.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babelify": "^7.3.0",
    "browserify": "^13.1.1",
    "del": "^2.2.2",
    "gulp": "^3.9.1",
    "gulp-copy": "^1.0.0",
    "gulp-less": "^3.3.0",
    "gulp-rename": "^1.2.2",
    "gulp-tap": "^0.1.3",
    "gulp-uglify": "^2.0.0",
    "gulp-util": "^3.0.7",
    "less-plugin-clean-css": "^1.5.1",
    "mocha": "^3.2.0",
    "run-sequence": "^1.2.2",
    "vinyl-buffer": "^1.0.0"
  }
